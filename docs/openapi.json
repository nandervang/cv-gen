{
  "openapi": "3.1.0",
  "info": {
    "title": "CV Generation Backend API",
    "description": "Professional CV generation API with multiple templates and export formats.\nDesigned for integration with consultant management systems and external applications.\n\n## Features\n- Multiple professional templates (Andervang Consulting, Modern, Classic, Creative)\n- PDF, HTML, and DOCX export formats\n- Flexible payload structure supporting multiple field naming conventions\n- Authentication via API key\n- Comprehensive error handling\n- Rate limiting for production use\n",
    "version": "2.0.0",
    "contact": {
      "name": "Andervang Consulting",
      "url": "https://andervang.com",
      "email": "niklas@andervang.com"
    },
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3001",
      "description": "Development server"
    },
    {
      "url": "https://andervang-cv.netlify.app/.netlify/functions",
      "description": "Production server (Netlify Functions)"
    }
  ],
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "paths": {
    "/health": {
      "get": {
        "summary": "Health Check",
        "description": "Check API server health and status",
        "operationId": "healthCheck",
        "security": [],
        "responses": {
          "200": {
            "description": "Server is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthResponse"
                },
                "example": {
                  "success": true,
                  "message": "CV Generation API is running",
                  "timestamp": "2025-10-14T08:44:13.217Z",
                  "version": "1.0.0",
                  "status": {
                    "server": "healthy",
                    "puppeteer": "ready",
                    "templates": "loaded"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/templates": {
      "get": {
        "summary": "List Available Templates",
        "description": "Retrieve all available CV templates with their metadata",
        "operationId": "listTemplates",
        "responses": {
          "200": {
            "description": "List of available templates",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TemplatesResponse"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "429": {
            "$ref": "#/components/responses/RateLimited"
          }
        }
      }
    },
    "/api/generate/complete": {
      "post": {
        "summary": "Generate Complete CV",
        "description": "Generate a CV in the specified format using provided data.\nSupports multiple payload formats for compatibility with different client systems.\n\n### Supported Field Name Variations\n- `profilePhoto` OR `profileImage` - Profile image data\n- `experience` OR `employment` OR `workExperience` - Work history\n- `skills` OR `competencies` OR `technicalSkills` - Skills and competencies\n- `title` OR `position` - Job title/position\n- `summary` OR `bio` - Professional summary\n- `website` OR `portfolio` - Personal website\n",
        "operationId": "generateCV",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CVGenerationRequest"
              },
              "examples": {
                "minimal": {
                  "summary": "Minimal payload",
                  "value": {
                    "personalInfo": {
                      "name": "John Doe"
                    },
                    "format": "pdf"
                  }
                },
                "consultant_manager_format": {
                  "summary": "Consultant manager compatible format",
                  "value": {
                    "personalInfo": {
                      "name": "Jane Doe",
                      "email": "jane.doe@example.com",
                      "phone": "+1-555-0123",
                      "location": "New York, NY",
                      "profilePhoto": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mP8/5+hHgAHggJ/PchI7wAAAABJRU5ErkJggg=="
                    },
                    "experience": [
                      {
                        "company": "TechCorp Inc",
                        "title": "Senior Developer",
                        "period": "2022-Present",
                        "description": "Lead development team"
                      }
                    ],
                    "skills": [
                      {
                        "category": "Frontend",
                        "items": [
                          "React",
                          "TypeScript",
                          "Vue.js"
                        ]
                      },
                      {
                        "category": "Backend",
                        "items": [
                          "Node.js",
                          "Python",
                          "PostgreSQL"
                        ]
                      }
                    ],
                    "format": "pdf"
                  }
                },
                "alternative_format": {
                  "summary": "Alternative field names format",
                  "value": {
                    "personalInfo": {
                      "name": "Jane Doe",
                      "email": "jane.doe@example.com",
                      "profileImage": "data:image/png;base64,test"
                    },
                    "employment": [
                      {
                        "company": "TechCorp",
                        "position": "Senior Developer",
                        "period": "2022-2025"
                      }
                    ],
                    "competencies": [
                      {
                        "category": "Frontend",
                        "items": [
                          "Vue.js",
                          "React"
                        ]
                      }
                    ],
                    "format": "pdf"
                  }
                },
                "comprehensive": {
                  "summary": "Full payload with all features",
                  "value": {
                    "personalInfo": {
                      "name": "Jane Alexandra Doe",
                      "email": "jane.doe@techcorp.com",
                      "phone": "+1 (555) 123-4567",
                      "location": "San Francisco, CA, USA",
                      "website": "https://janedoe.dev",
                      "linkedin": "https://linkedin.com/in/janedoe",
                      "github": "https://github.com/janedoe",
                      "summary": "Experienced Full-Stack Developer with 8+ years of expertise",
                      "profilePhoto": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mP8/5+hHgAHggJ/PchI7wAAAABJRU5ErkJggg=="
                    },
                    "experience": [
                      {
                        "company": "TechCorp Inc",
                        "title": "Senior Full-Stack Developer",
                        "period": "2022 - Present",
                        "location": "San Francisco, CA",
                        "description": "Lead a team of 5 developers in building scalable web applications",
                        "achievements": [
                          "Increased application performance by 40%",
                          "Led migration to microservices architecture"
                        ]
                      }
                    ],
                    "skills": [
                      {
                        "category": "Frontend Technologies",
                        "items": [
                          "React",
                          "Vue.js",
                          "TypeScript",
                          "Next.js"
                        ]
                      },
                      {
                        "category": "Backend Technologies",
                        "items": [
                          "Node.js",
                          "Python",
                          "PostgreSQL",
                          "MongoDB"
                        ]
                      }
                    ],
                    "education": [
                      {
                        "institution": "University of California, Berkeley",
                        "degree": "Bachelor of Science in Computer Science",
                        "period": "2014 - 2018",
                        "gpa": "3.8/4.0"
                      }
                    ],
                    "projects": [
                      {
                        "name": "E-Commerce Platform",
                        "description": "Full-stack solution with React and Node.js",
                        "technologies": [
                          "React",
                          "Node.js",
                          "PostgreSQL"
                        ],
                        "url": "https://github.com/janedoe/ecommerce"
                      }
                    ],
                    "certifications": [
                      {
                        "name": "AWS Certified Solutions Architect",
                        "issuer": "Amazon Web Services",
                        "date": "2023"
                      }
                    ],
                    "languages": [
                      {
                        "language": "English",
                        "proficiency": "Native"
                      },
                      {
                        "language": "Spanish",
                        "proficiency": "Fluent"
                      }
                    ],
                    "template": "andervang-consulting",
                    "format": "pdf"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "CV generated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CVGenerationResponse"
                },
                "example": {
                  "success": true,
                  "data": {
                    "fileUrl": "data:application/pdf;base64,JVBERi0xLjQKMSAwIG9iago...",
                    "format": "pdf",
                    "generatedAt": "2025-10-14T08:44:13.217Z",
                    "template": "andervang-consulting",
                    "filename": "CV_Jane_Doe.pdf",
                    "note": "Generated with ConsultantCVPayload format"
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/ValidationError"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "429": {
            "$ref": "#/components/responses/RateLimited"
          },
          "500": {
            "$ref": "#/components/responses/InternalError"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key",
        "description": "API key for authentication. \nDevelopment key: `dev-api-key-12345`\n"
      }
    },
